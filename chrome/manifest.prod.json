{
  "version": "1.0.0",
  "name": "Eastern Network",
  "manifest_version": 2,
  "description": "Best proxy service based in China, protect your privacy and unblock site when you surf web on your travels in China",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon-64.png",
    "48": "img/icon-128.png",
    "128": "img/icon-400.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [ "contextMenus", "tabs", "storage", "https://github.com/*" ],
  "content_security_policy": "default-src 'self'; script-src 'self'; style-src * 'unsafe-inline'; img-src 'self' data:;",
  "update_url": "http://clients2.google.com/service/update2/crx"
}
